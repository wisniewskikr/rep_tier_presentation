<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
 
<struts>

	<constant name="struts.devMode" value="true" />
 
	<package name="init" namespace="/" extends="struts-default">
		
		<action name="init">
			<result>pages/inputJsp.jsp</result>
		</action>
		
		<action name="inputOkAction" class="pl.kwi.actions.InputOkAction">
			<result name="SUCCESS">pages/outputJsp.jsp</result>
		</action>
		
		<action name="outputBackAction" class="pl.kwi.actions.OutputBackAction">			
			<result name="SUCCESS">pages/inputJsp.jsp</result>
		</action>
		
	</package>
 
</struts>